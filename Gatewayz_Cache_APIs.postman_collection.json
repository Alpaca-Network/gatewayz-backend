		{
			"name": "5. Chat & Inference (Cache Consumers)",
			"item": [
				{
					"name": "POST Chat Completions (OpenAI Compatible)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{api_key}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"model\": \"openai/gpt-4\",\n  \"messages\": [\n    {\n      \"role\": \"user\",\n      \"content\": \"Hello, how are you?\"\n    }\n  ],\n  \"stream\": false,\n  \"temperature\": 0.7,\n  \"max_tokens\": 150\n}"
						},
						"url": {
							"raw": "{{base_url}}/chat/completions",
							"host": ["{{base_url}}"],
							"path": ["chat", "completions"],
							"query": [
								{
									"key": "session_id",
									"value": "",
									"disabled": true,
									"description": "Optional: Chat session ID to save messages"
								}
							]
						},
						"description": "üî• MAIN INFERENCE ENDPOINT\n\nUses cache for:\n- Model validation (get_cached_models)\n- Butter.dev response caching (if enabled)\n- Provider client caching"
					},
					"response": []
				},
				{
					"name": "POST Messages (Anthropic Compatible)",
					"request": {
						"method": "POST",
						"header": [
