{
  "name": "gatewayz-js-hf",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript client for Gatewayz HuggingFace inference provider",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "gatewayz",
    "huggingface",
    "inference",
    "api",
    "client",
    "llm",
    "ai"
  ],
  "author": "Terragon Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/terragon-labs/gatewayz"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
