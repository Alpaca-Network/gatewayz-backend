{
  "dashboard": {
    "title": "Pricing Sync Scheduler - System Impact Dashboard",
    "description": "Monitor system resource usage and performance impact of the pricing sync scheduler",
    "tags": ["pricing", "scheduler", "performance", "system", "phase-6"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"gatewayz-api\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "auto"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 60, "color": "yellow"},
                {"value": 80, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"gatewayz-api\"} / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Resident Memory"
          },
          {
            "expr": "process_virtual_memory_bytes{job=\"gatewayz-api\"} / 1024 / 1024",
            "refId": "B",
            "legendFormat": "Virtual Memory"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "decmbytes",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1536, "color": "yellow"},
                {"value": 2048, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 3,
        "title": "Database Query Duration (Pricing Sync)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_sum{endpoint=~\".*pricing.*\"}[5m]) / rate(http_request_duration_seconds_count{endpoint=~\".*pricing.*\"}[5m])",
            "refId": "A",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 4,
        "title": "API Response Time",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"gatewayz-api\"}[5m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"gatewayz-api\"}[5m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"gatewayz-api\"}[5m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 5,
        "title": "HTTP Requests Per Second",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"gatewayz-api\"}[5m]))",
            "refId": "A",
            "legendFormat": "Total RPS"
          },
          {
            "expr": "sum by (status_code) (rate(http_requests_total{job=\"gatewayz-api\"}[5m]))",
            "refId": "B",
            "legendFormat": "{{status_code}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto",
              "stacking": {"mode": "normal"}
            },
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 6,
        "title": "Database Connection Pool",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "db_connection_pool_size{job=\"gatewayz-api\"}",
            "refId": "A",
            "legendFormat": "Pool Size"
          },
          {
            "expr": "db_connection_pool_active{job=\"gatewayz-api\"}",
            "refId": "B",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "db_connection_pool_idle{job=\"gatewayz-api\"}",
            "refId": "C",
            "legendFormat": "Idle Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "none",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["last", "mean"]}
        }
      },
      {
        "id": 7,
        "title": "Sync Impact on CPU (Correlation)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"gatewayz-api\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "rate(pricing_scheduled_sync_runs_total[5m]) * 1000",
            "refId": "B",
            "legendFormat": "Sync Events (scaled)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "CPU Usage %"},
              "properties": [
                {"id": "custom.axisPlacement", "value": "left"},
                {"id": "unit", "value": "percent"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Sync Events (scaled)"},
              "properties": [
                {"id": "custom.axisPlacement", "value": "right"},
                {"id": "unit", "value": "none"}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": []}
        }
      },
      {
        "id": 8,
        "title": "Provider API Response Times",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(pricing_provider_request_duration_seconds_sum[5m]) / rate(pricing_provider_request_duration_seconds_count[5m])",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 9,
        "title": "Database Query Errors",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(pricing_models_update_errors_total[5m])",
            "refId": "A",
            "legendFormat": "Update Errors"
          },
          {
            "expr": "rate(db_query_errors_total{operation=~\".*pricing.*\"}[5m])",
            "refId": "B",
            "legendFormat": "Query Errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "auto"
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "/.*Error.*/"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 10,
        "title": "Current Resource Usage Summary",
        "type": "stat",
        "gridPos": {"x": 0, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"gatewayz-api\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "CPU"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 60, "color": "yellow"},
                {"value": 80, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Memory (MB)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"gatewayz-api\"} / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Memory"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1536, "color": "yellow"},
                {"value": 2048, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Active DB Connections",
        "type": "stat",
        "gridPos": {"x": 12, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "db_connection_pool_active{job=\"gatewayz-api\"}",
            "refId": "A",
            "legendFormat": "Active"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 8, "color": "yellow"},
                {"value": 15, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Requests/sec",
        "type": "stat",
        "gridPos": {"x": 18, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"gatewayz-api\"}[5m]))",
            "refId": "A",
            "legendFormat": "RPS"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            }
          }
        }
      }
    ]
  },
  "overwrite": true,
  "folderId": 0,
  "folderUid": "",
  "message": "Pricing Sync System Impact Dashboard - Phase 6"
}
