{
  "dashboard": {
    "title": "Pricing Sync Scheduler - Health Dashboard",
    "description": "Monitor the automated pricing sync scheduler health, performance, and data freshness",
    "tags": ["pricing", "scheduler", "automation", "phase-6"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Scheduler Status",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "pricing_scheduler_enabled",
            "refId": "A",
            "legendFormat": "Enabled"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "Disabled"},
              {"type": "value", "value": "1", "text": "Enabled"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Sync Success Rate (24h)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(pricing_scheduled_sync_runs_total{status=\"success\"}[24h])) / sum(rate(pricing_scheduled_sync_runs_total[24h])) * 100",
            "refId": "A",
            "legendFormat": "Success Rate"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Last Sync Time",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "time() - max(pricing_last_sync_timestamp)",
            "refId": "A",
            "legendFormat": "Time Since Last Sync"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 21600, "color": "yellow"},
                {"value": 28800, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Total Syncs (24h)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "increase(pricing_scheduled_sync_runs_total[24h])",
            "refId": "A",
            "legendFormat": "Total Syncs"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 2, "color": "yellow"},
                {"value": 3, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Sync Success vs Failures (24h)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(pricing_scheduled_sync_runs_total{status=\"success\"}[5m])",
            "refId": "A",
            "legendFormat": "Success"
          },
          {
            "expr": "rate(pricing_scheduled_sync_runs_total{status=\"failed\"}[5m])",
            "refId": "B",
            "legendFormat": "Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto",
              "stacking": {"mode": "none"}
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Success"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Failed"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": []}
        }
      },
      {
        "id": 6,
        "title": "Sync Duration (seconds)",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(pricing_scheduled_sync_duration_seconds_sum[5m]) / rate(pricing_scheduled_sync_duration_seconds_count[5m])",
            "refId": "A",
            "legendFormat": "Avg Duration"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pricing_scheduled_sync_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p95 Duration"
          },
          {
            "expr": "histogram_quantile(0.99, rate(pricing_scheduled_sync_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "p99 Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 7,
        "title": "Models Updated Per Sync",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (provider) (rate(pricing_models_synced_total[5m]))",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto",
              "stacking": {"mode": "normal"}
            },
            "unit": "models/s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 8,
        "title": "Last Sync Timestamp Per Provider",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "time() - pricing_last_sync_timestamp",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 21600, "color": "yellow"},
                {"value": 28800, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["last"]}
        }
      },
      {
        "id": 9,
        "title": "Error Count (24h)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 20, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(pricing_scheduled_sync_runs_total{status=\"failed\"}[24h]))",
            "refId": "A",
            "legendFormat": "Total Errors"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 3, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Sync Interval Configuration",
        "type": "stat",
        "gridPos": {"x": 6, "y": 20, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "pricing_sync_interval_hours",
            "refId": "A",
            "legendFormat": "Interval"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "h",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Manual Trigger Count (24h)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 20, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(pricing_manual_sync_runs_total[24h]))",
            "refId": "A",
            "legendFormat": "Manual Triggers"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Providers Configured",
        "type": "stat",
        "gridPos": {"x": 18, "y": 20, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "count(count by (provider) (pricing_models_synced_total))",
            "refId": "A",
            "legendFormat": "Provider Count"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value",
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Error Rate Over Time",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "rate(pricing_scheduled_sync_runs_total{status=\"failed\"}[5m]) / rate(pricing_scheduled_sync_runs_total[5m]) * 100",
            "refId": "A",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "auto"
            },
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      }
    ]
  },
  "overwrite": true,
  "folderId": 0,
  "folderUid": "",
  "message": "Pricing Sync Scheduler Health Dashboard - Phase 6"
}
